{
  "name": "duckdb-datasource",
  "version": "0.2.1",
  "description": "DuckDB and MotherDuck datasource for grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "playwright test --trace on",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "postinstall": "patch-package"
  },
  "author": "Motherduck",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/eslint-config": "latest",
    "@grafana/plugin-e2e": "latest",
    "@grafana/tsconfig": "2.0.0",
    "@playwright/test": "^1.41.2",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.8",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "sql-formatter-plus": "^1.3.6",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.0",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@grafana/data": "latest",
    "@grafana/experimental": "latest",
    "@grafana/plugin-ui": "^0.9.4",
    "@grafana/runtime": "latest",
    "@grafana/ui": "latest",
    "patch-package": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tslib": "2.5.3"
  },
  "overrides": {
    "@grafana/data": "latest",
    "@grafana/experimental": "latest",
    "@grafana/runtime": "latest",
    "@grafana/ui": "latest",
    "@emotion/css": "11.11.2"
  },
  "resolutions": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "packageManager": "npm@10.5.0"
}
